<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.haji-umrah" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Haji Umrah</name>
    <description>
        Panduan Haji dan Umrah
    </description>
    <author email="adnanlutfi92@gmail.com" href="https://github.com/lutfiadnan92">
        lutfiadnan92
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
